
@ Unit@ = /Unit%.Unit@;

% True = /Bool%.True;

<@>@ Maybe@ = /Maybe%.Maybe@;

<@>@ Get@ = /Process%.Get@;
<@>@ Put@ = /Process%.Put@;

@ Stdio@ = /Stdio%.Stdio@;

@ String@ = /String%.String@;

# The 'cat' program to cat stdin.
Stdio@ Cat = (Get@<Maybe@<String@>> stdin, Put@<String@> stdout) {
  Maybe@<String@> line := stdin;
  line.?(
    just: {
      Unit@ _ := stdout(line.just);
      Cat(stdin, stdout);
    },
    nothing: !(True)
  );
};

Cat;
