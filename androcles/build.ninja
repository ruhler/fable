
# androcles type
build build/androcles/type.o: cc androcles/type.cc
build build/androcles/type_test.o: cc androcles/type_test.cc
build build/test_androcles_type: ld $
    build/androcles/type.o $
    build/androcles/type_test.o $
    build/error.o
build build/test_androcles_type_passed: run build/test_androcles_type

# androcles value
build build/androcles/value.o: cc androcles/value.cc
build build/androcles/value_test.o: cc androcles/value_test.cc
build build/test_androcles_value: ld $
    build/androcles/type.o $
    build/androcles/value.o $
    build/androcles/value_test.o $
    build/error.o
build build/test_androcles_value_passed: run build/test_androcles_value

# androcles function
build build/androcles/function/access_expr.o: cc androcles/function/access_expr.cc
build build/androcles/function/application_expr.o: cc androcles/function/application_expr.cc
build build/androcles/function/expr.o: cc androcles/function/expr.cc
build build/androcles/function/function.o: cc androcles/function/function.cc
build build/androcles/function/select_expr.o: cc androcles/function/select_expr.cc
build build/androcles/function/struct_expr.o: cc androcles/function/struct_expr.cc
build build/androcles/function/union_expr.o: cc androcles/function/union_expr.cc
build build/androcles/function/var_expr.o: cc androcles/function/var_expr.cc
build build/androcles/function.o: cc androcles/function.cc
build build/androcles/function_test.o: cc androcles/function_test.cc

build build/test_androcles_function: ld $
    build/androcles/function/access_expr.o $
    build/androcles/function/application_expr.o $
    build/androcles/function/expr.o $
    build/androcles/function/function.o $
    build/androcles/function/select_expr.o $
    build/androcles/function/struct_expr.o $
    build/androcles/function/union_expr.o $
    build/androcles/function/var_expr.o $
    build/androcles/function.o $
    build/androcles/function_test.o $
    build/androcles/type.o $
    build/androcles/value.o $
    build/error.o
build build/test_androcles_function_passed: run build/test_androcles_function

