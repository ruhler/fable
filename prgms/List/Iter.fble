
<@>@ Iter@ = /Iter%.Iter@;
<@>% Has = /Iter%.Has;
<@>% End = /Iter%.End;

<@>@ List@ = /List%.List@;

@ Unit@ = /Unit%.Unit@;

# Each --
#   An iterator that visits each element of the given list in order.
<@ T@>(List@<T@>) { Iter@<T@>; } Each = <@ T@>(List@<T@> l) {
  (Unit@ _) {
    l.?(cons: Has<T@>(l.cons.head, Each<T@>(list.cons.tail)), nil: End<T@>);
  };
};

@(Each);
