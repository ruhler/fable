
@ Decimal@ = /Digits%.Decimal@;

@ Rat@ = /Rat%.Rat@;

<@>@ List@ = /List%.List@;

# Int --
#   Create a Rat@ from a positive decimal integer literal.
#
# For example: Rat@ 42 = Int|42;
(List@<Decimal@>) { Rat@; } Int = (List@<Decimal@> ds) {
  Rat@(/Int/Lit%.Int(ds), /Int/Lit%.Int|1);
};

@(Int);
