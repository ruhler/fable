
% True = /Bool%.True;
% False = /Bool%.False;
<@>@ Eq@ = /Eq%.Eq@;
<@>@ S@ = /List%.S@;

# Eq --
#   Implementation of Eq@ for S@. See documentation of /Eq%.Eq@.
<@ T@>(Eq@<T@>) { Eq@<S@<T@>>; } Eq = <@ T@>(Eq@<T@> eq)(S@<T@> a, S@<T@> b) {
  a.?(
    cons: b.?(
      cons: {
        eq(a.cons.head, b.cons.head).?(
          true: Eq<T@>(eq)(a.cons.tail, b.cons.tail),
          false: False);
      },
      nil: False),
    nil: b.?(cons: False, nil: True));
};

@(Eq);
