@(StdLib, UBNat) {
  StdLib { @(Unit@, Bool, List); };
  Bool { @(Bool@, Not); };
  List { @(P@, S@, S0, ConsS); };
  UBNat { @(
      Nat@,
      0, 1, 2, 3, 4, 5, 6, 7, Lit2,
      EqNat, Lt, Le, Gt, Ge,
      Inc, Dec, Add, Sub, Mul, Div, Mod
    );
  };

  @ TestName@ = +(
    Unit@ Eq_0_0, Unit@ Eq_0_1, Unit@ Eq_1_1,
    Unit@ Le_5_5, Unit@ Le_5_7, Unit@ Le_7_5,
    Unit@ Lt_5_5, Unit@ Lt_5_7, Unit@ Lt_7_5,
    Unit@ Ge_5_5, Unit@ Ge_5_7, Unit@ Ge_7_5,
    Unit@ Gt_5_5, Unit@ Gt_5_7, Unit@ Gt_7_5,
    Unit@ Inc_5,
    Unit@ Dec_5,
    Unit@ Add_5_7,
    Unit@ Sub_7_5, Unit@ Sub_5_7,
    Unit@ Mul_5_7,
    Unit@ Div_6_3, Unit@ Div_10_3,
    Unit@ Mod_6_3, Unit@ Mod_10_3
  );

  @ TestFailure@ = *(TestName@ name, Nat@ a, Nat@ b);

  (TestName@, Bool@, (Unit@){S@<TestFailure@>;}){S@<TestFailure@>;} T =
    (TestName@ name, Bool@ p, (Unit@){S@<TestFailure@>;} rest) {
      # Just use 0 for the context arguments for now to avoid introducing
      # a union for the test failure type.
      ?(p; true: rest(Unit@()),
           false: ConsS<TestFailure@>(TestFailure@(name, 0, 0), rest(Unit@())));
    };

  (TestName@, Nat@, Nat@, (Unit@){S@<TestFailure@>;}){S@<TestFailure@>;} TEq =
    (TestName@ name, Nat@ a, Nat@ b, (Unit@){S@<TestFailure@>;} rest) {
      ?(EqNat(a, b);
          true: rest(Unit@()),
          false: ConsS<TestFailure@>(TestFailure@(name, a, b), rest(Unit@())));
    };

  (Unit@){S@<TestFailure@>;} Test = (Unit@ _) {
    Unit@ _ <- T(TestName@(Eq_0_0: Unit@()), EqNat(0, 0));
    Unit@ _ <- T(TestName@(Eq_0_1: Unit@()), Not(EqNat(0, 1)));
    Unit@ _ <- T(TestName@(Eq_1_1: Unit@()), EqNat(1, 1));
    Unit@ _ <- T(TestName@(Le_5_5: Unit@()), Le(5, 5));
    Unit@ _ <- T(TestName@(Le_5_7: Unit@()), Le(5, 7));
    Unit@ _ <- T(TestName@(Le_7_5: Unit@()), Not(Le(7, 5)));
    Unit@ _ <- T(TestName@(Lt_5_5: Unit@()), Not(Lt(5, 5)));
    Unit@ _ <- T(TestName@(Lt_5_7: Unit@()), Lt(5, 7));
    Unit@ _ <- T(TestName@(Lt_7_5: Unit@()), Not(Lt(7, 5)));
    Unit@ _ <- T(TestName@(Ge_5_5: Unit@()), Ge(5, 5));
    Unit@ _ <- T(TestName@(Ge_5_7: Unit@()), Not(Ge(5, 7)));
    Unit@ _ <- T(TestName@(Ge_7_5: Unit@()), Ge(7, 5));
    Unit@ _ <- T(TestName@(Gt_5_5: Unit@()), Not(Gt(5, 5)));
    Unit@ _ <- T(TestName@(Gt_5_7: Unit@()), Not(Gt(5, 7)));
    Unit@ _ <- T(TestName@(Gt_7_5: Unit@()), Gt(7, 5));
    Unit@ _ <- TEq(TestName@(Inc_5: Unit@()), 6, Inc(5));
    Unit@ _ <- TEq(TestName@(Dec_5: Unit@()), 4, Dec(5));
    Unit@ _ <- TEq(TestName@(Dec_5: Unit@()), 4, Dec(5));
    Unit@ _ <- TEq(TestName@(Add_5_7: Unit@()), Lit2(1, 2), Add(5, 7));
    Unit@ _ <- TEq(TestName@(Sub_5_7: Unit@()), 0, Sub(5, 7));
    Unit@ _ <- TEq(TestName@(Mul_5_7: Unit@()), Lit2(3, 5), Mul(5, 7));
    Unit@ _ <- TEq(TestName@(Div_6_3: Unit@()), 2, Div(6, 3));
    Unit@ _ <- TEq(TestName@(Div_10_3: Unit@()), 3, Div(Lit2(1, 0), 3));
    Unit@ _ <- TEq(TestName@(Mod_6_3: Unit@()), 0, Mod(6, 3));
    Unit@ _ <- TEq(TestName@(Mod_10_3: Unit@()), 1, Mod(Lit2(1, 0), 3));
    S0<TestFailure@>;
  };

  @(TestName@, TestFailure@, Test);
};
