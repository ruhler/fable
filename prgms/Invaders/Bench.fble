
@ Key@ = /App%.Key@;
@ Event@ = /App%.Event@;

% I = /Int/Int/Lit%.I;
% D = /Int/Int/Lit%.D;

@ Unit@ = /Unit%.Unit@;
% Unit = /Unit%.Unit;

# The number of iterations is chosen so that the benchmark takes on the
# order of 1 minute to run. The target goal for invaders is 60 frames a
# second. We're not quite there yet.
Unit@! Bench = /App/Benchmark%.Benchmark(
  /Invaders/App%, I(D|32), Event@(key_down: Key@(q: Unit)));

@(Bench);
