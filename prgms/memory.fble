
@ Unit@ = *();
@ Num@ = +(Unit@ Z, Num@ S);

[Num@] { Unit@; } f = [Num@ x] {
  ?(x; Z: Unit@(), S: f[x.S]);
};

Num@ 0 = Num@(Z: Unit@());
Num@ 4 = Num@(S: Num@(S: Num@(S: Num@(S: 0))));
Num@ 8 = Num@(S: Num@(S: Num@(S: Num@(S: 4))));
Num@ 12 = Num@(S: Num@(S: Num@(S: Num@(S: 8))));
Num@ 16 = Num@(S: Num@(S: Num@(S: Num@(S: 12))));
Num@ 20 = Num@(S: Num@(S: Num@(S: Num@(S: 16))));
Num@ 24 = Num@(S: Num@(S: Num@(S: Num@(S: 20))));

f[0];

