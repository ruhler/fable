mtype List<T> {
  using Unit<;> { Unit; };

  # P - A list of 1 or more elements: T+
  struct P(T head, S tail);

  # S - A list of 0 or more elements: T*
  union S(Unit nil, P cons); 

  # InitP - Return all but the last element in 'list'.
  func InitP(P list; S);

  # LastP - Return the last element in 'list'.
  func LastP(P list; T);
};
