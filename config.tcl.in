
# The build directory
set ::builddir @builddir@

# The top level directory containing the fble source code. This is the
# directory that contains the 'configure' script.
set ::srcdir @srcdir@

# The directory to install includes to.
set ::includedir @includedir@

# The directory to install libraries to.
set ::libdir @libdir@

# The directory to install binaries to.
set ::bindir @bindir@

# The directory to install manual pages to.
set ::mandir @mandir@
