
<@>@ List@ = /List%.List@;
<@>% List = /List%.List;
<@>% Concat = /List%.Concat;

<@>@ Maybe@ = /Maybe%.Maybe@;

<@>@ Get@ = /Process%.Get@;
<@>@ Put@ = /Process%.Put@;

@ Stdio@ = /Stdio%.Stdio@;

@ String@ = /String%.String@;

@ Test@ = /Test%.Test@;

# Import to test compilation.
% _ = /DataFlow/Counter%;
% _ = /Fble/Bench%;
% _ = /Fble/StackSmasher%;
% _ = /Invaders/App%;
% _ = /Invaders/ScreenshotApp%;
% _ = /Snake%;
% _ = /Stdio/Cat%;

List@<Test@> AllTests = Concat<Test@>(List<List@<Test@>>([
  /Bool/Tests%.Tests,
  /GameOfLife/Tests%.Tests,
  /Hwdg/Tests%.Tests,
  /Int/Int/Tests%.Tests,
  /List/Tests%.Tests,
  /Map/Tests%.Tests,
  /Maybe/Tests%.Tests,
  /Md5/Tests%.Tests,
  /Primes/Tests%.Tests,
  /Sat/Tests%.Tests,
  /String/Tests%.Tests,
  /Sudoku/Tests%.Tests,
  /Test/Tests%.Tests,
  /TicTacToe/Tests%.Tests
]));

Stdio@ Main = (Get@<Maybe@<String@>> in, Put@<String@> out) {
  /Test%.RunTests(out, AllTests);
};

Main;
