
{ /Unit%; @(Unit@, Unit); };
{ /Int/Int%; @(Int@, Lit2, 0, 1, 2, 3, 4, 5, 6, 7); };
{ /GameOfLife%; @(Coord@, World@, World, StepN); };

(Int@, Int@) { Coord@; } C = (Int@ x, Int@ y) { Coord@(x, y); };

World@ acorn = World([
  C(0, 1),
  C(1, 3),
  C(2, 0), C(2, 1), C(2, 4), C(2, 5), C(2, 6)
]);

Unit@! Bench = $({
  # 65 iterations takes about a minute to run currently.
  World@ _ = StepN(Lit2(6, 5), acorn);
  Unit;
});

@(Bench);
