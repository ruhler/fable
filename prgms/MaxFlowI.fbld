interf MaxFlowI<Node> {
  import @ { ListM; UBNatM; };
  import ListM { S; };
  import UBNatM { Nat; };

  struct Edge(Node src, Node dst);

  # Return the max flow from source node src to destination node dst in the
  # graph defined by the given list of edges.
  func MaxFlow(S<Edge> edges, Node src, Node dst ; Nat);
}
