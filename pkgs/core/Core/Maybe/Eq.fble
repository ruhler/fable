
% True = /Core/Bool%.True;
% False = /Core/Bool%.False;
<@>@ Eq@ = /Core/Eq%.Eq@;
<@>@ Maybe@ = /Core/Maybe%.Maybe@;

<@ T@>(Eq@<T@>) { Eq@<Maybe@<T@>>; }
Eq = <@ T@>(Eq@<T@> eq)(Maybe@<T@> a, Maybe@<T@> b) {
  a.?(nothing: b.?(nothing: True, : False));
  b.?(nothing: False);
  eq(a.just, b.just);
};

@(Eq);
