
% False = /Core/Bool%.False;

<@>@ Eq@ = /Core/Eq%.Eq@;

<@>@ Result@ = /Fbld/Result%.Result@;

<@ T@>(Eq@<T@>) { Eq@<Result@<T@>>; }
Eq = <@ T@>(Eq@<T@> eq)(Result@<T@> a, Result@<T@> b) {
  a.?(
    ok: b.?(ok: eq(a.ok, b.ok), err: False),
    err: b.?(ok: False, err: /Fbld/Text/Eq%.Eq(a.err, b.err)));
};

@(Eq);
