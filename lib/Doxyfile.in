# Add @sideeffects tag that can be used in doc comments.
ALIASES = "sideeffects=@par Side Effects:^^"
JAVADOC_AUTOBRIEF = YES

INPUT = @srcdir@/lib
FILE_PATTERNS = *.h *.c
OPTIMIZE_OUTPUT_FOR_C = YES

QUIET = YES
