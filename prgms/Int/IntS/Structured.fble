@ IntS@ = /Int/IntS%.IntS@;
% Tag = /Structure/Union%.Tag;
% Enum = /Structure/Union%.Enum;
% Union = /Structure/Union%.Union;
<@>@ Structured@ = /Structured%.Structured@;

# Structured --
#   Instance of Structured@ for IntS@.
Structured@<IntP@> Structured = (IntS@ x) {
  x.?(
    0: Enum(Tag|0),
    p: Union(Tag|1, /Int/IntP/Structured%.Structured(x.p)));
};

@(Structured);
