
{ /List%; @(S@, S, Concat); };
{ /Maybe%; @(Maybe@); };
{ /Process%; @(Get@, Put@); };
{ /Stdio%; @(Stdio@); };
{ /String%; @(Str@); };
{ /Test%; @(Test@); };

S@<Test@> AllTests = Concat<Test@>(S<S@<Test@>>([
  /GameOfLife/Tests%.Tests,
  /Int/Tests%.Tests,
  /List/Tests%.Tests,
  /Map/Tests%.Tests,
  /Md5/Tests%.Tests,
  /Nat/Tests%.Tests,
  /Primes/Tests%.Tests,
  /Sudoku/Tests%.Tests,
  /Test%.Tests,
  /TicTacToe/Tests%.Tests
]));

Stdio@ Main = (Get@<Maybe@<Str@>> in, Put@<Str@> out) {
  /Test%.RunTests(out, AllTests);
};

Main;
