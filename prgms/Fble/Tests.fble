
<@>@ S@ = /List%.S@;
<@>% S = /List%.S;
<@>% Concat = /List%.Concat;

<@>@ Maybe@ = /Maybe%.Maybe@;

<@>@ Get@ = /Process%.Get@;
<@>@ Put@ = /Process%.Put@;

@ Stdio@ = /Stdio%.Stdio@;

@ Str@ = /String%.Str@;

@ Test@ = /Test%.Test@;

# Import to test compilation.
% _ = /Fble/Bench%;
% _ = /Fble/StackSmasher%;
% _ = /Invaders/ScreenshotApp%;
% _ = /Snake%;
% _ = /Stdio/Cat%;

S@<Test@> AllTests = Concat<Test@>(S<S@<Test@>>([
  /GameOfLife/Tests%.Tests,
  /Int/Int/Tests%.Tests,
  /List/Tests%.Tests,
  /Map/Tests%.Tests,
  /Md5/Tests%.Tests,
  /Primes/Tests%.Tests,
  /Sudoku/Tests%.Tests,
  /Test%.Tests,
  /TicTacToe/Tests%.Tests
]));

Stdio@ Main = (Get@<Maybe@<Str@>> in, Put@<Str@> out) {
  /Test%.RunTests(out, AllTests);
};

Main;
