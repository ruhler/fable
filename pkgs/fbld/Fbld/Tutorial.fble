
% Str = /Core/String%.Str;

% Define = /Fbld/Define%;

@ Markup@ = /Fbld/Markup%.Markup@;
% Block = /Fbld/Markup%.Block;
% Entry = /Fbld/Markup%.Entry;

Markup@ Markup = Block[
  Entry(Str|'tutorial', Define.DefB[Str|'name', Str|'content']|'
@doc[$name]
@FbleVersion (@BuildStamp)

$$content'),
  Entry(Str|'exercise', Define.DefB[Str|'content']|'
$$content')
];

@(Markup);
