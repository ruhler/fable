
@ Bool@ = /Bool%.Bool@;
% True = /Bool%.True;
% False = /Bool%.False;
% Chars = /Char%.Chars;
@ Test@ = /Test%.Test@;
% Test = /Test%.Test;
% TestSuite = /Test%.TestSuite;
<@>% AssertEquals = /Test%.AssertEquals;

% Bit = /Digits%.Bit;
% EqBit = /Digits/Eq%.EqBit;

Test@ Tests = TestSuite(Str|Digits, List<Test@>[
  TestSuite(Str|EqBit, List<Test@>[
    Test(Str|00, !({
      Bool@ wnt = True;
      Bool@ got = EqBit(Bit.0, Bit.0);
      AssertEquals<Bool@>(/Bool/Eq%.Eq, /Bool/Show%.Show, wnt, got);
    })),

    Test(Str|01, !({
      Bool@ wnt = False;
      Bool@ got = EqBit(Bit.0, Bit.1);
      AssertEquals<Bool@>(/Bool/Eq%.Eq, /Bool/Show%.Show, wnt, got);
    })),

    Test(Str|10, !({
      Bool@ wnt = False;
      Bool@ got = EqBit(Bit.1, Bit.0);
      AssertEquals<Bool@>(/Bool/Eq%.Eq, /Bool/Show%.Show, wnt, got);
    })),

    Test(Str|11, !({
      Bool@ wnt = True;
      Bool@ got = EqBit(Bit.1, Bit.1);
      AssertEquals<Bool@>(/Bool/Eq%.Eq, /Bool/Show%.Show, wnt, got);
    }))])
]);

@(Tests);
