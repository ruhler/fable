
<@>@ Maybe@ = /Core/Maybe%.Maybe@;

@ String@ = /Core/String%.String@;

@ Unit@ = /Core/Unit%.Unit@;

# Package type for access to internals of Stdio@ abstract type.
@ _@ = %(/Core/Stdio%);

# Stdio@ --
#   Internal interface for standard input and output operations.
<<@>@>@ Stdio@ = <<@>@ M@> { *(
    M@<Maybe@<String@>> in,
    (String@) { M@<Unit@>; } out,
    (String@) { M@<Unit@>; } err
  );
};


@(_@, Stdio@);
