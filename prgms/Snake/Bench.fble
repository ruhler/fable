
@ Key@ = /App%.Key@;
@ Event@ = /App%.Event@;

% Int = /Int/Lit%.Int;

@ Unit@ = /Unit%.Unit@;
% Unit = /Unit%.Unit;

Unit@! Bench = /App/Benchmark%.Benchmark(
  # 1300 is around the number of iterations the Snake app runs for. Don't try
  # to run more, otherwise we could get deadlock.
  /Snake%, Int|1300, Event@(key_down: Key@(q: Unit)));

@(Bench);
