
@ Unit@ = /Unit%.Unit@;
% Unit = /Unit%.Unit;

@ Bit@ = +(Unit@ 0, Unit@ 1);
Bit@ 0 = Bit@(0: Unit);
Bit@ 1 = Bit@(1: Unit);

% Bit = @(0, 1);

(Bit@, Bit@) { Bit@; } And = (Bit@ a, Bit@ b) {
  a.?(0: 0, 1: b);
};

(Bit@, Bit@) { Bit@; } Or = (Bit@ a, Bit@ b) {
  a.?(0: b, 1: 1);
};

@(Bit@, Bit, And, Or);
