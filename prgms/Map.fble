
{ /Maybe%; @(Maybe@); };

# A generic map interface.
#   M@ - the map type.
#   K@ - the key type.
#   V@ - the value type.
<@,@,@>@ Map@ = <@ M@, @ K@, @ V@> {
  *(@<M@> M@,
    M@ Empty,
    (M@, K@) { Maybe@<V@>; } Lookup,
    (M@, K@, (Maybe@<V@>){ V@; }) { M@; } InsertWith,
    (M@, K@, V@) { M@; } Insert
   );
};
    
@(Map@);
