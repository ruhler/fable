
<@>@ List@ = /Core/List%.List@;

@ Command@ = /Fbld/Command%.Command@;
<@>@ Result@ = /Fbld/Result%.Result@;
@ StringL@ = /Fbld/StringL%.StringL@;

<@>% Run = /Fbld/Parse/M%.Run;

# Parse inline structured text.
(StringL@) { Result@<List@<Command@>>; }
Inline = (StringL@ in) {
  Run(/Fbld/Parse/Inline%.Inline, in);
};

# Parse block structured text.
(StringL@) { Result@<List@<Command@>>; }
Block = (StringL@ in) {
  Run(/Fbld/Parse/Block%.Block, in);
};

@(Inline, Block);
