
<@>@ List@ = /Core/List%.List@;

@ String@ = /Fbld/Types%.String@;
@ Command@ = /Fbld/Types%.Command@;
<@>@ Result@ = /Fbld/Types%.Result@;

<@>% Run = /Fbld/Parse/M%.Run;

# Parse inline structured text.
(String@) { Result@<List@<Command@>>; }
Inline = (String@ in) {
  Run(/Fbld/Parse/Inline%.Inline, in);
};

# Parse block structured text.
(String@) { Result@<List@<Command@>>; }
Block = (String@ in) {
  Run(/Fbld/Parse/Block%.Block, in);
};

@(Inline, Block);
