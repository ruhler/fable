
<@>@ Eq@ = /Eq%.Eq@;

@ Int@ = /Int/Int%.Int@;

@ Rat@ = /Rat%.Rat@;

Eq@<Rat@> Gt = (Rat@ a, Rat@ b) {
  Eq@<Int@> gt = {
    a.d.?(n: {
      b.d.?(n: /Int/Int/Eq%.Gt);
      /Int/Int/Eq%.Lt;
    });
    /Int/Int/Eq%.Gt;
  };

  gt(/Int/Int%.Mul(a.n, b.d), /Int/Int%.Mul(b.n, a.d));
};

Eq@<Rat@> Lt = (Rat@ a, Rat@ b) {
  Eq@<Int@> lt = {
    a.d.?(n: {
      b.d.?(n: /Int/Int/Eq%.Lt);
      /Int/Int/Eq%.Gt;
    });
    /Int/Int/Eq%.Lt;
  };

  lt(/Int/Int%.Mul(a.n, b.d), /Int/Int%.Mul(b.n, a.d));
};

Eq@<Rat@> Le = (Rat@ a, Rat@ b) {
  Eq@<Int@> le = {
    a.d.?(n: {
      b.d.?(n: /Int/Int/Eq%.Le);
      /Int/Int/Eq%.Ge;
    });
    /Int/Int/Eq%.Le;
  };

  le(/Int/Int%.Mul(a.n, b.d), /Int/Int%.Mul(b.n, a.d));
};

@(Gt, Lt, Le);
