# @@fble-test@@ no-error
@ Unit@ = *();
@ Bool@ = +(Unit@ true, Unit@ false);
Bool@ True = Bool@(true: Unit@());
Bool@ False = Bool@(false: Unit@());

@ Foo@ = *((Bool@) { Bool@; } f);

Foo@ x = {
  # Verify we can use bind syntax with struct value. It's a funny case, but
  # it's legal according to the spec.
  Bool@ x <- Foo@;
  x.?(true: False, false: True);
};
x.f(True).false;
