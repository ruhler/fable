
% True = /Core/Bool%.True;

<@>@ List@ = /Core/List%.List@;

<@>@ Maybe@ = /Core/Maybe%.Maybe@;

@ Stdio@ = /Stdio%.Stdio@;
@ IO@ = /Stdio%.IO@;

@ String@ = /Core/String%.String@;

@ Unit@ = /Core/Unit%.Unit@;

# The 'cat' program to cat stdin.
Stdio@ Cat = (IO@ io, List@<String@> args) {
  Maybe@<String@> line := io.stdin;
  line.?(
    just: {
      Unit@ _ := io.stdout(line.just);
      Cat(io, args);
    },
    nothing: !(True)
  );
};

Cat;
