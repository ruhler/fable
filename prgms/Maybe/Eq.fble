
@ Bool@ = /Bool%.Bool@;
% True = /Bool%.True;
% False = /Bool%.False;

<@>@ Maybe@ = /Maybe%.Maybe@;

<@ T@>((T@, T@) { Bool@; }, Maybe@<T@>, Maybe@<T@>) { Bool@; }
Eq = <@ T@>((T@, T@) { Bool@; } eq, Maybe@<T@> a, Maybe@<T@> b) {
  a.?(
    just: b.?(just: eq(a.just, b.just), nothing: False),
    nothing: b.?(just: False, nothing: True));
};

@(Eq);
