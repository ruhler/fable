
@ Test@ = /Core/Test%.Test@;

@ Int@ = /Core/Int%.Int@;
% Neg = /Core/Int%.Neg;
% Int = /Core/Int/Lit%.Int;

# Note: Iteration order is not quite what you expect.
# Negative before zero before positive, yes, but evens come before odds.
Test@ Tests = /Core/Map/Test%.Test</Core/Int/Map%.Map@, Int@>(
  /Core/Int/Map%, /Core/Int/Eq%.Eq, /Core/Int/Show%.Show, 
  Neg(Int|4), Int|5, Int|7);

@(Tests);
