
@(StdLib: &StdLib);
@(Test: &Test);
@(UBNat: &UBNat);

@(Primes: &Primes);

@(Bits: &Bits);
@(Md5: &Md5);

StdLib { @(Char, List); };
Char { @(Char@); };
List { @(S@, S4, Concat); };
Test { @(Test@); };

S@<Test@> AllTests = Concat<Test@>(S4<S@<Test@>>(
  Test.Tests,
  UBNat.Tests.Tests,
  Primes.Tests.Tests,
  Md5.Tests.Tests
));

(S@<Char@>+ out) {
  Test.RunTests(out, AllTests);
};
