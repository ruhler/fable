
@ Bool@ = /Core/Bool%.Bool@;
% True = /Core/Bool%.True;
% False = /Core/Bool%.False;
<@>% List = /List%.List;
% Str = /String%.Str;
@ Test@ = /Core/Test%.Test@;
% Test = /Core/Test%.Test;
% TestSuite = /Core/Test%.TestSuite;
<@>% AssertEquals = /Core/Test%.AssertEquals;

% Bit = /Digits%.Bit;
% EqBit = /Digits/Eq%.EqBit;

Test@ Tests = TestSuite(Str|Digits, List<Test@>[
  TestSuite(Str|EqBit, List<Test@>[
    Test(Str|00, !({
      Bool@ wnt = True;
      Bool@ got = EqBit(Bit.0, Bit.0);
      AssertEquals<Bool@>(/Core/Bool/Eq%.Eq, /Core/Bool/Show%.Show, wnt, got);
    })),

    Test(Str|01, !({
      Bool@ wnt = False;
      Bool@ got = EqBit(Bit.0, Bit.1);
      AssertEquals<Bool@>(/Core/Bool/Eq%.Eq, /Core/Bool/Show%.Show, wnt, got);
    })),

    Test(Str|10, !({
      Bool@ wnt = False;
      Bool@ got = EqBit(Bit.1, Bit.0);
      AssertEquals<Bool@>(/Core/Bool/Eq%.Eq, /Core/Bool/Show%.Show, wnt, got);
    })),

    Test(Str|11, !({
      Bool@ wnt = True;
      Bool@ got = EqBit(Bit.1, Bit.1);
      AssertEquals<Bool@>(/Core/Bool/Eq%.Eq, /Core/Bool/Show%.Show, wnt, got);
    }))])
]);

@(Tests);
