
# This file is used as input to fble-profiles-test to test profiling
# instrumentation of .fble code.

@ Unit@ = *();
Unit@ Unit = Unit@();

@ Bool@ = +(Unit@ true, Unit@ false);
Bool@ True = Bool@(true: Unit);
Bool@ False = Bool@(false: Unit);

(Bool@) { Bool@; } Not = (Bool@ x) {
  x.?(true: False, false: True);
};

Bool@ t = Not(False);
Bool@ f = Not(True);
Bool@ f2 = Not(True);

# Make sure we don't output blocks per field here, which we used to do in the
# past.
% _ = @(t, f, f2);

(Bool@) { Bool@; } Id = (Bool@ x) {
  x;
};

Bool@ e1 := !(Id(True)), Bool@ e2 := !(Id(False));
Bool@ e3 := !(Id(True));
Unit@ _ := !(e3.true);

Unit@ ~ get, C;
(Unit@) { Unit@!; } D = (Unit@ x) { !(x); };
(Unit@) { Unit@!; } B = (Unit@ x) { C(x); };
(Unit@) { Unit@!; } A = (Unit@ x) {
  Unit@ b := B(x);
  D(b);
};
Unit@ _a := A(Unit);

!(@());
