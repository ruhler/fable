
@ Char@ = /Char%.Char@;
% Chars = /Char%.Chars;

<@>% Append = /List%.Append;

<@>@ Maybe@ = /Maybe%.Maybe@;

@ Str@ = /String%.Str@;
% Str = /String%.Str;


<@ T@>((T@) { Str@; }, Maybe@<T@>) { Str@; }
Print = <@ T@>((T@) { Str@; } print, Maybe@<T@> m) {
  m.?(
    just: Append<Char@>(Str(Chars|'Just:'), print(m.just)),
    nothing: Str(Chars|'Nothing'));
};

@(Print);
