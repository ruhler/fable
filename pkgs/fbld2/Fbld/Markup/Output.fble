
<@>% Append = /Core/List%.Append;

@ String@ = /Core/String%.String@;
% Str = /Core/String%.Str;

@ Markup@ = /Fbld/Markup%.Markup@;
@ Element@ = /Fbld/Markup%.Element@;

(Markup@) { String@; }
Output = (Markup@ m) {
  m.?(nil: Str|'');
  Element@ e = m.cons.head;
  String@ head = e.plain.text;
  String@ tail = Output(m.cons.tail);
  Append(head, tail);
};

@(Output);
