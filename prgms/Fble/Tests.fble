
% Chars = /Char%.Chars;

<@>@ Maybe@ = /Maybe%.Maybe@;

<@>@ Get@ = /Process%.Get@;
<@>@ Put@ = /Process%.Put@;

@ Stdio@ = /Stdio%.Stdio@;

@ String@ = /String%.String@;

@ Test@ = /Test%.Test@;
% TestSuite = /Test%.TestSuite;

# Import to test compilation.
% _ = /DataFlow/Counter%;
% _ = /Fble/Bench%;
% _ = /Fble/StackSmasher%;
% _ = /Hwdg/App%;
% _ = /Invaders/App%;
% _ = /Invaders/ScreenshotApp%;
% _ = /Snake%;
% _ = /Stdio/Cat%;

Test@ AllTests = TestSuite(Chars|Fble, [
  /Bool/Tests%.Tests,
  /Digits/Tests%.Tests,
  /GameOfLife/Tests%.Tests,
  /Fblf/Tests%.Tests,
  /Hwdg/Tests%.Tests,
  /Int/Int/Tests%.Tests,
  /List/Tests%.Tests,
  /Map/Tests%.Tests,
  /Maybe/Tests%.Tests,
  /Md5/Tests%.Tests,
  /Primes/Tests%.Tests,
  /Sat/Tests%.Tests,
  /String/Tests%.Tests,
  /Sudoku/Tests%.Tests,
  /Test/Tests%.Tests,
  /TicTacToe/Tests%.Tests
]);

Stdio@ Main = (Get@<Maybe@<String@>> in, Put@<String@> out) {
  /Test%.RunTests(out, AllTests);
};

Main;
