
@ Int@ = /Int/Int%.Int@;
% Enum = /Structure/Union%.Enum;
% Union = /Structure/Union%.Union;
<@>@ Structured@ = /Structured%.Structured@;

# Structured --
#   Instance of Structured@ for Int@.
Structured@<Int@> Structured = (Int@ x) {
  x.?(
    n: Union|10(/Int/IntP/Structured%.Structured(x.n)),
    0: Enum|0,
    p: Union|11(/Int/IntP/Structured%.Structured(x.p)));
};

@(Structured);
