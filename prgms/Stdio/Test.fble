
@ Unit@ = /Core/Unit%.Unit@;

% True = /Core/Bool%.True;

<@>@ List@ = /Core/List%.List@;

@ Stdio@ = /Stdio%.Stdio@;
@ IO@ = /Stdio%.IO@;

@ String@ = /Core/String%.String@;
% Str = /Core/String%.Str;

Stdio@ Test = (IO@ io, List@<String@> args) {
  # Output a single line to test that we don't drop trailing outputs.
  Unit@ _ := io.stdout(Str|'PASSED');
  !(True);
};

Test;
