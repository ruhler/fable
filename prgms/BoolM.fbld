module BoolM(BoolI) {
  import @ { UnitM; }
  import UnitM { Unit; };

  union Bool(Unit true, Unit false);

  func True( ; Bool) {
    Bool:true(Unit());
  };

  func False( ; Bool) {
    Bool:false(Unit());
  };
 
  func Not(Bool a; Bool) {
    ?(a; False(), True());
  };
 
  func And(Bool a, Bool b; Bool) {
    ?(a; b, False());
  };

  func Or(Bool a, Bool b; Bool) {
    ?(a; True(), b);
  };
};
