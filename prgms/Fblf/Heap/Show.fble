

<@,@>% Map = /List%.Map;
<@>@ Show@ = /Show%.Show@;
@ String@ = /String%.String@;

@ Bit@ = /Digits%.Bit@;
@ Heap@ = /Fblf/Heap%.Heap@;
% GetAllBits = /Fblf/Heap%.GetAllBits;

Show@<Heap@> Show = (Heap@ heap) {
  /String%.Concat(Map<Bit@, String@>(GetAllBits(heap), /Digits/Show%.ShowBit));
};

@(Show);
