
@(StdLib: &StdLib);
@(Test: &Test);
@(UBNat: &UBNat);
@(Primes: &Primes);
@(Bits: &Bits);

@(List_Tests_: &List_Tests_);

@(Md5_: &Md5_);
@(Md5_Tests_: &Md5_Tests_);

StdLib { @(Char, List); };
Char { @(Char@); };
List { @(S@, S5, Concat); };
Test { @(Test@); };

S@<Test@> AllTests = Concat<Test@>(S5<S@<Test@>>(
  Test.Tests,
  List_Tests_.Tests,
  UBNat.Tests.Tests,
  Primes.Tests.Tests,
  Md5_Tests_.Tests
));

(S@<Char@>+ out) {
  Test.RunTests(out, AllTests);
};
