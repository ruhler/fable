
@ IntP@ = /Int/IntP%.IntP@;
% Tag = /Structure/Union%.Tag;
% Enum = /Structure/Union%.Enum;
% Union = /Structure/Union%.Union;
<@>@ Structured@ = /Structured%.Structured@;

# Structured --
#   Instance of Structured@ for IntP@.
Structured@<IntP@> Structured = (IntP@ x) {
  x.?(
    1: Enum(Tag|0),
    2p0: Union(Tag|10, Structured(x.2p0)),
    2p1: Union(Tag|11, Structured(x.2p1)));
};

@(Structured);
