
% True = /Bool%.True;
% False = /Bool%.False;
<@>@ Eq@ = /Eq%.Eq@;
<@>@ List@ = /List%.List@;

# Eq --
#   Implementation of Eq@ for List@. See documentation of /Eq%.Eq@.
<@ T@>(Eq@<T@>) { Eq@<List@<T@>>; }
Eq = <@ T@>(Eq@<T@> eq)(List@<T@> a, List@<T@> b) {
  a.?(
    cons: b.?(
      cons: {
        eq(a.cons.head, b.cons.head).?(
          true: Eq<T@>(eq)(a.cons.tail, b.cons.tail),
          false: False);
      },
      nil: False),
    nil: b.?(cons: False, nil: True));
};

@(Eq);
