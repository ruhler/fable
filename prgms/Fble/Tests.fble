  /Digits/Tests%.Tests,
  /GameOfLife/Tests%.Tests,
  /Hwdg/Tests%.Tests,
  /Int/Int/Tests%.Tests,
  /List/Tests%.Tests,
  /Map/Tests%.Tests,
  /Maybe/Tests%.Tests,
