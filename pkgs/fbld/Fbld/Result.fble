
@ Text@ = /Fbld/Text%.Text@;

# A result of an operation that may fail.
<@>@ Result@ = <@ T@> { +(T@ ok, Text@ err); };

<@ T@>(T@) { Result@<T@>; } Ok = <@ T@>(T@ x) {
  Result@<T@>(ok: x);
};

<@ T@>(Text@) { Result@<T@>; } Err = <@ T@>(Text@ msg) {
  Result@<T@>(err: msg);
};

@(Result@, Ok, Err);
