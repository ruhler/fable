
{ Bool%; @(Bool@, True); };
{ Maybe%; @(Maybe@); };
{ String%; @(Str@); };

# The 'cat' program to cat stdin.
(Maybe@<Str@>-, Str@+){ Bool@!; } cat = (Maybe@<Str@>- stdin, Str@+ stdout) {
  Maybe@<Str@> line := stdin();
  ?(line;
      just: {
        Str@ _ := stdout(line.just);
        cat(stdin, stdout);
      },
      nothing: $(True));
};

cat;
