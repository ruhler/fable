
<<@>@>@ Monad@ = /Core/Monad%.Monad@;

# World@ --
#   An abstract type representing the state of the world.
# TODO: Make this private to /Core/Monad/IO%:
# @ World@ = *().%(/Core/Monad/IO%);
@ World@ = *();

% State = /Core/Monad/State%<World@>;

# IO@ -- 
#   An IO monad.
<@>@ IO@ = State.State@;
<@>@ R@ = State.R@;

# Monad --
#   The Monad instance for IO@.
Monad@<IO@> Monad = State.Monad;

@(IO@, World@, R@, Monad);
