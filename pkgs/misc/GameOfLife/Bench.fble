
@ Key@ = /App/App%.Key@;
@ Event@ = /App/App%.Event@;

% Int = /Core/Int/Lit%.Int;

@ Unit@ = /Core/Unit%.Unit@;
% Unit = /Core/Unit%.Unit;

Unit@! Bench = /App/Benchmark%.Benchmark(
  # The GameOfLife App is set to run for 200 iterations. Use a little less
  # than that for the benchmark to avoid deadlock.
  /GameOfLife/App%, Int|180, Event@(key_down: Key@(q: Unit)));

@(Bench);
