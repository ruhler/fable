
% True = /Core/Bool%.True;
% False = /Core/Bool%.False;
<@>@ Eq@ = /Core/Eq%.Eq@;
<@>@ Maybe@ = /Maybe%.Maybe@;

<@ T@>(Eq@<T@>) { Eq@<Maybe@<T@>>; }
Eq = <@ T@>(Eq@<T@> eq)(Maybe@<T@> a, Maybe@<T@> b) {
  a.?(
    just: b.?(just: eq(a.just, b.just), nothing: False),
    nothing: b.?(just: False, nothing: True));
};

@(Eq);
