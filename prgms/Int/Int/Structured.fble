
@ Int@ = /Int/Int%.Int@;
% Tag = /Structure/Union%.Tag;
% Enum = /Structure/Union%.Enum;
% Union = /Structure/Union%.Union;
<@>@ Structured@ = /Structured%.Structured@;

# Structured --
#   Instance of Structured@ for Int@.
Structured@<Int@> Structured = (Int@ x) {
  x.?(
    n: Union(Tag|10, /Int/IntP/Structured%.Structured(x.n)),
    0: Enum(Tag|0),
    p: Union(Tag|11, /Int/IntP/Structured%.Structured(x.p)));
};

@(Structured);
